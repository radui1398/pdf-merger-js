{
  "name": "@radui1398/pdf-merger",
  "version": "5.1.2",
  "description": "merge multiple PDF documents, or parts of them, to a new PDF document",
  "type": "module",
  "engines": {
    "node": ">=16"
  },
  "main": "./index.js",
  "types": "./index.d.ts",
  "browser": "./browser.js",
  "bin": {
    "pdf-merge": "cli.js"
  },
  "scripts": {
    "standard": "standard",
    "standard:fix": "standard --fix",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest --detectOpenHandles",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch --forceExit --detectOpenHandles"
  },
  "standard": {
    "env": [
      "jest",
      "browser"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/radui1398/pdf-merger-js.git"
  },
  "keywords": [
    "pdf",
    "merge"
  ],
  "author": "nbesli",
  "license": "MIT",
  "dependencies": {
    "@cantoo/pdf-lib": "^2.4.2",
    "commander": "^11.1.0"
  },
  "devDependencies": {
    "fs-extra": "^11.2.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jsdom": "^23.0.1",
    "pdf-diff": "^0.1.1",
    "standard": "^17.1.0"
  },
  "directories": {
    "test": "test"
  },
  "bugs": {
    "url": "https://github.com/radui1398/pdf-merger-js/issues"
  },
  "homepage": "https://github.com/radui1398/pdf-merger-js#readme"
}
